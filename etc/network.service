[Unit]
Description=connect to wifi

[Service]
Type=oneshot
ExecStart=/usr/bin/sleep 1
ExecStart=/usr/bin/ifconfig wlan0 10.12.111.36/20 up
ExecStart=/usr/bin/route add default gw 10.12.111.254
ExecStart=/bin/bash -c "/usr/bin/wpa_passphrase munie 8806250014 > /tmp/wpa.conf"
ExecStart=/usr/bin/wpa_supplicant -iwlan0 -c /tmp/wpa.conf

[Install]
WantedBy=multi-user.target
